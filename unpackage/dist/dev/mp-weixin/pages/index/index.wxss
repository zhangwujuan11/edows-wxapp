/* 行为相关颜色 */
page .cate-section {
  position: relative;
  z-index: 5;
  border-radius: 16rpx 16rpx 0 0;
  margin-top: -20rpx;
}
page .carousel-section .titleNview-placing {
  padding-top: 0;
  height: 0;
}
page .carousel-section .carousel .carousel-item {
  padding: 0;
}
page .carousel-section .swiper-dots {
  left: 45rpx;
  bottom: 40rpx;
}
page {
  background: #399eff;
}
.topBg {
  width: 100%;
  background: url(data:image/jpeg;base64,) center no-repeat;
  background-size: contain;
}
.yd_top {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.jifen {
  width: 15%;
  text-align: center;
  margin-right: 15rpx;
  margin-top: 15rpx;
}
.jifen image {
  width: 40rpx;
  height: 40rpx;
}
.jifen view {
  font-size: 24rpx;
  color: #fff;
}
.showIcon {
  position: fixed;
  right: 1%;
  bottom: 20rpx;
  z-index: 10;
  width: 180rpx;
  height: 180rpx;
}
.showIcon image {
  width: 180rpx;
  height: 180rpx;
}
.m-t {
  margin-top: 16rpx;
}
.mp-search-box {
  position: relative;
  width: 100%;
  padding: 30rpx 0 10rpx;
}
.mp-search-box .m-icon-search {
  position: absolute;
  display: block;
  width: 40rpx;
  height: 40rpx;
  left: 70rpx;
  top: 42rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  z-index: 1;
}
.mp-search-box .ser-input {
  position: relative;
  display: block;
  width: 92%;
  margin: 0 auto;
  flex: 1;
  height: 70rpx;
  line-height: 70rpx;
  text-indent: 100rpx;
  font-size: 28rpx;
  color: #606266;
  border-radius: 70rpx;
  background: #ffffff;
}
/* 头部 轮播图 */
.carousel-section {
  position: relative;
}
.carousel-section .titleNview-placing {
  height: 25px;
  padding-top: 44px;
  box-sizing: content-box;
}
.carousel-section .titleNview-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 426rpx;
  transition: .4s;
}
.carousel {
  width: 710rpx;
  padding-top: 20rpx;
  margin: 0 auto;
  height: 280rpx;
}
.carousel .carousel-item {
  width: 710rpx;
  height: 256rpx;
  overflow: hidden;
}
.carousel image {
  width: 710rpx;
  height: 256rpx;
  border-radius: 20rpx;
}
/* 分类 */
.cate-section {
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  padding: 30rpx 22rpx;
  background: #fff;
  /* 原图标颜色太深,不想改图了,所以加了透明度 */
}
.cate-section .cate-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  color: #303133;
}
.cate-section image {
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 14rpx;
  border-radius: 50%;
  opacity: .7;
  box-shadow: 4rpx 4rpx 20rpx rgba(250, 67, 106, 0.3);
}
.ad-1 {
  width: 100%;
  height: 210rpx;
  padding: 10rpx 0;
  background: #fff;
}
.ad-1 image {
  width: 100%;
  height: 100%;
}
/* 秒杀专区 */
.seckill-section {
  padding: 4rpx 30rpx 24rpx;
}
.seckill-section .inner-section {
  background: #f3f8fe;
  border-radius: 20rpx;
}
.seckill-section .s-header {
  position: relative;
  align-items: center;
  height: 86rpx;
}
.seckill-section .s-header .pic-floor1 {
  width: 282rpx;
  height: 50rpx;
}
.seckill-section .s-header .pic-floor2 {
  width: 262rpx;
  height: 50rpx;
}
.seckill-section .s-header .pic-floor3,
.seckill-section .s-header .pic-floor4 {
  width: 222rpx;
  height: 50rpx;
}
.seckill-section .s-header .line-h {
  height: 4rpx;
  width: 424rpx;
  position: absolute;
  right: 0;
  top: 4rpx;
  background: #218fe6;
}
.seckill-section .s-header .line-h2 {
  width: 464rpx;
}
.seckill-section .s-header .s-img {
  width: 140rpx;
  height: 30rpx;
}
.seckill-section .s-header .tip {
  font-size: 28rpx;
  color: #909399;
  margin: 0 20rpx 0 40rpx;
}
.seckill-section .s-header .timer {
  display: inline-block;
  width: 40rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  margin-right: 14rpx;
  font-size: 26rpx;
  color: #fff;
  border-radius: 2px;
  background: rgba(0, 0, 0, 0.8);
}
.seckill-section .s-header .icon-you {
  font-size: 32rpx;
  color: #909399;
  flex: 1;
  text-align: right;
}
.seckill-section .s-header .icon-you:before {
  content: '';
}
.seckill-section .s-header .icon-you:after {
  content: "\E606";
}
.seckill-section .floor-list {
  white-space: nowrap;
}
.seckill-section .scoll-wrapper {
  padding: 0 10px 10px;
  display: flex;
  align-items: flex-start;
}
.seckill-section .floor-item {
  width: 150rpx;
  margin-right: 18rpx;
  font-size: 26rpx;
  color: #303133;
  line-height: 1.8;
}
.seckill-section .floor-item image {
  width: 150rpx;
  height: 150rpx;
  text-align: center;
  border-radius: 20rpx;
  border: 4rpx solid #2a9fe6;
}
.seckill-section .floor-item .title {
  padding: 14rpx 0 0 0;
  font-size: 24rpx;
  text-align: center;
  color: #303133;
  line-height: 38rpx;
  height: 60rpx;
}
.seckill-section .floor-item .price {
  font-weight: 600;
  color: #f04c41;
}
.f-header {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 140rpx;
  padding: 6rpx 30rpx 0;
  color: #fff;
  text-align: center;
}
.f-header image {
  flex-shrink: 0;
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.f-header .tit {
  font-size: 34rpx;
}
.f-header .tit2 {
  font-size: 24rpx;
  color: #909399;
}
.f-header .icon-you {
  font-size: 34rpx;
  color: #909399;
}
/* 团购楼层 */
.group-section .g-swiper {
  height: 650rpx;
  padding-bottom: 30rpx;
}
.group-section .g-swiper-item {
  width: 100%;
  padding: 0 30rpx;
  display: flex;
}
.group-section image {
  width: 100%;
  height: 460rpx;
  border-radius: 4px;
}
.group-section .g-item {
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.group-section .left {
  flex: 1.2;
  margin-right: 24rpx;
}
.group-section .left .t-box {
  padding-top: 20rpx;
}
.group-section .right {
  flex: 0.8;
  flex-direction: column-reverse;
}
.group-section .right .t-box {
  padding-bottom: 20rpx;
}
.group-section .t-box {
  height: 160rpx;
  font-size: 30rpx;
  color: #303133;
  line-height: 1.6;
}
.group-section .price {
  font-weight: 600;
  color: #f04c41;
}
.group-section .m-price {
  font-size: 26rpx;
  text-decoration: line-through;
  color: #909399;
  margin-left: 8rpx;
}
.group-section .pro-box {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: 28rpx;
  padding-right: 10rpx;
}
.group-section .progress-box {
  flex: 1;
  border-radius: 10px;
  overflow: hidden;
  margin-right: 8rpx;
}
/* 分类推荐楼层 */
.hot-floor {
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.hot-floor .floor-img-box {
  width: 100%;
  height: 320rpx;
  position: relative;
}
.hot-floor .floor-img-box:after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(rgba(255, 255, 255, 0.06) 30%, #f8f8f8);
}
.hot-floor .floor-img {
  width: 100%;
  height: 100%;
}
.hot-floor .floor-list {
  white-space: nowrap;
  padding: 20rpx;
  padding-right: 50rpx;
  border-radius: 6rpx;
  margin-top: -140rpx;
  margin-left: 30rpx;
  background: #fff;
  box-shadow: 1px 1px 5px rgba(0, 0, 0, 0.2);
  position: relative;
  z-index: 1;
}
.hot-floor .scoll-wrapper {
  display: flex;
  align-items: flex-start;
}
.hot-floor .floor-item {
  width: 200rpx;
  display: inline-block;
  margin-right: 20rpx;
  font-size: 24rpx;
  color: #303133;
  line-height: 1.8;
}
.hot-floor .floor-item image {
  border-radius: 6rpx;
}
.hot-floor .floor-item .price {
  color: #f04c41;
}
.hot-floor .more {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  flex-shrink: 0;
  width: 180rpx;
  height: 180rpx;
  border-radius: 6rpx;
  background: #f3f3f3;
  font-size: 28rpx;
  color: #909399;
}
.hot-floor .more text:first-child {
  margin-bottom: 4rpx;
}
/* 猜你喜欢 */
.guess-section {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
}
.guess-section .guess-item {
  display: flex;
  flex-direction: column;
  width: 330rpx;
  margin-bottom: 30rpx;
  border-radius: 20rpx;
  background: #fff;
}
.guess-section .guess-item:nth-child(2n+1) {
  margin-right: 30rpx;
}
.guess-section .image-wrapper {
  width: 330rpx;
  height: 330rpx;
  border-radius: 20rpx 20rpx 0 0;
  overflow: hidden;
  background: #fff;
}
.guess-section .image-wrapper image {
  width: 330rpx;
  height: 330rpx;
  opacity: 1;
  border-radius: 20rpx 20rpx 0 0;
}
.guess-section .inner-guess {
  padding: 20rpx;
}
.guess-section .title {
  padding: 14rpx 0 0 0;
  font-size: 26rpx;
  color: #303133;
  line-height: 38rpx;
  height: 60rpx;
}
.guess-section .price {
  font-weight: 600;
  padding-top: 14rpx;
  font-size: 32rpx;
  color: #f04c41;
  line-height: 1;
}

