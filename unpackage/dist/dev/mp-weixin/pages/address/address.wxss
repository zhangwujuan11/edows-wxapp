page {
  padding-bottom: 120rpx;
  background: #f8f8f8;
}
.content {
  position: relative;
}
.list {
  display: flex;
  align-items: center;
  padding: 24rpx;
  background: #fff;
  position: relative;
  margin-top: 30rpx;
}
.wrapper {
  display: flex;
  flex-direction: column;
  flex: 1;
}
.address-box {
  display: flex;
  align-items: center;
}
.address-box .address {
  font-size: 30rpx;
  color: #909399;
}
.u-box {
  font-size: 30rpx;
  color: #303133;
  margin: 18rpx 0;
}
.u-box .name {
  margin-right: 30rpx;
}
.u-box .tag {
  font-size: 20rpx;
  color: #fff;
  margin-left: 20rpx;
  background: #339cfe;
  border-radius: 40rpx;
  padding: 4rpx 20rpx;
  line-height: 1;
}
.m-icon-edit {
  display: flex;
  align-items: center;
  width: 36rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  margin-left: 30rpx;
}
.m-icon-addr {
  width: 46rpx;
  height: 46rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  margin: 0 20rpx 0 0;
}
.add-btn {
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  bottom: 16rpx;
  z-index: 95;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 690rpx;
  height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #339cfe;
  border-radius: 10rpx;
  /* box-shadow: 1px 2px 5px rgba(219, 63, 96, 0.4);	 */
}

