<view><view class="yt-list"><view class="yt-list-cell b-b"><text class="cell-tit clamp">订单编号</text><text class="cell-tip">{{orderInfo.orderSn}}</text></view></view><view class="goods-section"><block wx:for="{{productList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="g-item"><image src="{{item.imgPath}}"></image><view class="right"><text class="title clamp">{{item.productName}}</text><view class="spec"><block wx:for="{{item.attrValueVOList}}" wx:for-item="subItem" wx:for-index="subIndex" wx:key="subIndex"><text>{{''+subItem.attrValue+''}}</text></block></view><view class="price-box"><text class="price">{{"￥"+item.productPrice}}</text><text class="number">{{"x "+item.productNum}}</text></view></view></view></block></view><view class="yt-list"><view data-event-opts="{{[['tap',[['toggleMask',['show']]]]]}}" class="yt-list-cell b-b" bindtap="__e"><text class="cell-tit clamp">退款原因</text><text class="cell-tip active">{{''+refundReason+''}}</text><text class="cell-more wanjia wanjia-gengduo-d"></text></view></view><view class="footer"><view class="price-content"><text>退款金额</text><text class="price-tip">￥</text><text class="price">{{total}}</text></view><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">申请退款</text></view><view data-event-opts="{{[['tap',[['toggleMask',['$event']]]]]}}" class="{{['mask',maskState===0?'none':maskState===1?'show':'']}}" bindtap="__e"><view data-event-opts="{{[['tap',[['stopPrevent',['$event']]]]]}}" class="mask-content" catchtap="__e"><block wx:for="{{couponList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="coupon-item"><view data-event-opts="{{[['tap',[['chooseReson',['$0'],[[['couponList','',index]]]]]]]}}" class="con" bindtap="__e"><view class="left"><text class="title">{{item.title}}</text></view></view></view></block></view></view></view>