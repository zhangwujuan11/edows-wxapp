/* 行为相关颜色 */
page {
  background: #f8f8f8;
}
.container {
  position: relative;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background: #f8f8f8;
}
.topImg {
  position: relative;
  text-align: center;
  padding: 100rpx 0;
}
.topImg .serviceNo {
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    font-size: 30rpx;
}
.topImg image {
    width: 165rpx;
    height: 165rpx;
}
.wrapper {
  position: relative;
  z-index: 90;
  padding-bottom: 40rpx;
}
.back-btn {
  position: absolute;
  left: 40rpx;
  z-index: 9999;
  padding-top: 25px;
  top: 40rpx;
  font-size: 40rpx;
  color: #303133;
}
.left-top-sign {
  font-size: 120rpx;
  color: #f8f8f8;
  position: relative;
  left: -16rpx;
}
.right-top-sign {
  position: absolute;
  top: 80rpx;
  right: -30rpx;
  z-index: 95;
}
.right-top-sign:before, .right-top-sign:after {
    display: block;
    content: "";
    width: 400rpx;
    height: 80rpx;
    background: #b4f3e2;
}
.right-top-sign:before {
    -webkit-transform: rotate(50deg);
        -ms-transform: rotate(50deg);
            transform: rotate(50deg);
    border-radius: 0 50px 0 0;
}
.right-top-sign:after {
    position: absolute;
    right: -198rpx;
    top: 0;
    -webkit-transform: rotate(-50deg);
        -ms-transform: rotate(-50deg);
            transform: rotate(-50deg);
    border-radius: 50px 0 0 0;
    /* background: pink; */
}
.left-bottom-sign {
  position: absolute;
  left: -270rpx;
  bottom: -320rpx;
  border: 100rpx solid #d0d1fd;
  border-radius: 50%;
  padding: 180rpx;
}
.welcome {
  position: relative;
  left: 50rpx;
  top: -90rpx;
  font-size: 46rpx;
  color: #555;
  text-shadow: 1px 0px 1px rgba(0, 0, 0, 0.3);
}
.input-content {
  padding: 0 60rpx;
}
.input-item {
  position: relative;
  padding: 0 30rpx;
  border-bottom: 1px solid #c8e4ff;
  height: 120rpx;
  border-radius: 4px;
  margin-bottom: 50rpx;
}
.input-item:last-child {
    margin-bottom: 0;
}
.input-item .tit {
    position: absolute;
    left: 0;
    top: 40rpx;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    vertical-align: middle;
    background: url(/static/user.png) no-repeat;
    background-size: contain;
}
.input-item .tit.icon-password {
    background: url(/static/pwd.png) no-repeat;
    background-size: contain;
}
.input-item input {
    height: 120rpx;
    text-indent: 50rpx;
    font-size: 30rpx;
    color: #303133;
    width: 100%;
}
.confirm-btn {
  width: 630rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 10rpx;
  margin-top: 160rpx;
  color: #fff;
  font-size: 32rpx;
}
.forget-section {
  font-size: 26rpx;
  width: 630rpx;
  text-align: right;
  margin: 40rpx auto 0;
}
.register-section {
  position: absolute;
  left: 0;
  bottom: 50rpx;
  width: 100%;
  font-size: 26rpx;
  color: #606266;
  text-align: center;
}
.register-section .m-icon-index {
    width: 46rpx;
    height: 46rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}
.register-section text {
    display: inline-block;
    vertical-align: middle;
    margin-left: 10rpx;
}

